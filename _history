ral to the right\n//0 to move the center of the spiral up\n//1 to move the center of the spiral down\n//2 to reverse the rotation of the spiral\n\nfunction fill(x: any, y: any) {\n    led.plot(x, y);\n}\n\nlet interval = 10;\nlet chA = 0;\nlet r = 0.5;\nlet dencity = 0.1;\nlet length = Math.PI * 8;\nlet centerX = 2;\nlet centerY = 2;\nlet speed = -0.2;\n\nbasic.forever(function () {\n    basic.clearScreen();\n    for (let a = 0; a < length; a += Math.PI * dencity) {\n        fill(Math.round(centerX + Math.cos(a + chA) * a * r),\n            Math.round(centerY + Math.sin(a + chA) * a * r));\n    }\n    chA += speed;\n    basic.pause(interval);\n})\n\ninput.onButtonPressed(Button.A, function () {\n    centerX -= 1\n})\n\ninput.onButtonPressed(Button.B, function () {\n    centerX += 1\n})\n\ninput.onPinPressed(TouchPin.P0, function () {\n    centerY -= 1\n})\n\ninput.onPinPressed(TouchPin.P1, function () {\n    centerY += 1\n})\n\ninput.onPinPressed(TouchPin.P2, function () {\n    speed = -speed\n})","README.md":"","pxt.json":"{\n    \"name\": \"Spinning spiralka generator :3\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\",\n    \"languageRestriction\": \"javascript-only\"\n}\n"}}],"shares":[{"id":"04218-93892-35277-34000","timestamp":1731150322234},{"id":"18395-12869-01024-01883","timestamp":1731151552070},{"id":"18394-94930-38840-66543","timestamp":1731151599008}],"lastSaveTime":1731179469753}